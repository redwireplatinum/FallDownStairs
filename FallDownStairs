function SCRIPT_YIJW85_FAKESCRIPT() -- MainFrame.Draggable 
	local script = Instance.new('LocalScript')
	script.Parent = MainFrame
	script.Parent.Draggable = true

end
coroutine.resume(coroutine.create(SCRIPT_YIJW85_FAKESCRIPT))
function SCRIPT_VACM90_FAKESCRIPT() -- Explode.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Explode
	script.Parent.MouseButton1Down:connect(function()
		for i, plr in ipairs(game.Players:GetPlayers()) do
			local plrname = script.Parent.Parent.Plrname.Text
			if string.match (string.lower(plr.Name), string.lower(plrname)) then
			if workspace:FindFirstChild(plr.Name) then
				game.ReplicatedStorage.Events.GenerateExplosion:FireServer(100, 100000000, plr.Character.Torso.Position)
			end
			end
		end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_VACM90_FAKESCRIPT))
function SCRIPT_CFZP80_FAKESCRIPT() -- stealhats.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = stealhats
	script.Parent.MouseButton1Down:connect(function()
	for i, plr in ipairs(game.Players:GetPlayers()) do
		game.ReplicatedStorage.Events.TransferHats:FireServer(workspace[plr.Name])
	end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_CFZP80_FAKESCRIPT))
function SCRIPT_OJGU88_FAKESCRIPT() -- velocity.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = velocity
	script.Parent.MouseButton1Down:connect(function()
	for i, child in ipairs(workspace:GetDescendants()) do
		if child:IsA("BasePart") then
			if child.Velocity ~= Vector3.new(0, 0, 0) then
				child.Velocity = Vector3.new(0, 0, 0)
			end
		end
	end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_OJGU88_FAKESCRIPT))
function SCRIPT_VFAZ67_FAKESCRIPT() -- invisible.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = invisible
	script.Parent.MouseButton1Down:connect(function()
		local torso = game.Players.LocalPlayer.Character.Torso
		if torso.Transparency == 0 then
		game.ReplicatedStorage.Events.SetPlayerTransparency:FireServer(1)
		elseif torso.Transparency == 1 then
		game.ReplicatedStorage.Events.SetPlayerTransparency:FireServer(0)
		end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_VFAZ67_FAKESCRIPT))

end
