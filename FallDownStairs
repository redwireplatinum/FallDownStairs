-- Scripts:
function SCRIPT_KQRM85_FAKESCRIPT() -- ItemGet.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = ItemGet
	script.Parent.MouseButton1Down:connect(function()
		for i, Item in ipairs(game.ReplicatedStorage.ShopItems:GetChildren()) do
			local itemname = script.Parent.Parent.ItemName.Text
			if string.match (string.lower(Item.Name), string.lower(itemname)) then
				game.ReplicatedStorage.Events.PlayerShopPurchase:FireServer(Item.Name, 0)
			end
		end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_KQRM85_FAKESCRIPT))
function SCRIPT_IBAQ76_FAKESCRIPT() -- Plinko.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Plinko
	script.Parent.MouseButton1Down:connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Dest_Plinko.CFrame
	end)

end
coroutine.resume(coroutine.create(SCRIPT_IBAQ76_FAKESCRIPT))
function SCRIPT_IWKD81_FAKESCRIPT() -- Pyramid.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Pyramid
	script.Parent.MouseButton1Down:connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Dest_Pyramid.CFrame
	end)

end
coroutine.resume(coroutine.create(SCRIPT_IWKD81_FAKESCRIPT))
